{
  "name": "GShop",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "prepare": "husky",
    "pod": "cd ios && pod install && cd ..",
    "apk": "cd android && ./gradle clean && ./gradle assembleRelease",
    "aab": "cd android && ./gradle clean && ./gradle bundleRelease",
    "archive-ios": "xcodebuild -workspace ios/GShop.xcworkspace -scheme GShop -sdk iphoneos -configuration Release archive -archivePath ios/build/GShop.xcarchive"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.0.0",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/drawer": "^6.7.2",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@shopify/react-native-skia": "^1.5.1",
    "@shopify/restyle": "^2.4.4",
    "@tanstack/react-query": "^5.59.16",
    "add": "^2.0.6",
    "axios": "^1.7.7",
    "babel-plugin-module-resolver": "^5.0.2",
    "bottlejs": "^2.0.1",
    "lottie-react-native": "^7.1.0",
    "react": "18.3.1",
    "react-native": "0.76.0",
    "react-native-dotenv": "^3.4.11",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.20.2",
    "react-native-reanimated": "^3.16.1",
    "react-native-safe-area-context": "^4.12.0",
